# Optional: OpenAI API key for summarization (leave blank to disable LLM)
OPENAI_API_KEY=

# Output directory for JSON results
OUTPUT_DIR=./data

# Milliseconds to wait between requests (simple rate-limit)
RATE_LIMIT_MS=1000

# Custom user-agent for scraping (optional)
SCRAPER_USER_AGENT=web-scraping-agent/1.0 (+https://example.com)